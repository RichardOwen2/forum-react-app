{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "off",
        "no-alert": "off",
        "no-underscore-dangle": "off",
        "no-shadow": "off",
        "react/jsx-props-no-spreading": "off",
        "react/forbid-prop-types": "off",
        "jsx-a11y/label-has-associated-control": "off"
    }
}
